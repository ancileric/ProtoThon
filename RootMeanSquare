import math
count = 0
total = 0
while(1):
    num =  input("Enter a number: ")
    if(num == "done"):
        break
    try:
        number = float(num)
        if (total == 0):
            total = number 
        else:
            total = total + number
        count = count + 1
        #print(total,count)
        
        
    except:
        print("That's not a number dumbo")
        continue
a = total/count        
print(math.sqrt(a))
